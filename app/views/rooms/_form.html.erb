<%= form_for(@room) do |f| %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :quest_start_time %><br />
    <%= f.datetime_field :quest_start_time %>
  </div>
   <div class="field">
    <%= f.label :quest_end_time %><br />
    <%= f.datetime_field :quest_end_time %>
  </div> 
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
