<div class="page-header">
<h1>シートの編集</h1>
</div>
<%= form_for(@room, url: room_path(@room.uuid), html: {role: "form"}) do |f| %>
<%= render 'create_form', f: f %>
<% end %>

<h3>シートの削除</h3>
<p>
<%= link_to 'このシートを削除する', room_path(@room.uuid), method: :delete, class: "btn btn-danger", data: { confirm: 'シートを削除しますか？' } %>
</p>
