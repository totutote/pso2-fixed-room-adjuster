{
  "name": "pso2-fixed-room-adjuster",
  "scripts": {
  },
  "env": {
    "DEPLOY_TASKS": {
      "required": true
    },
    "LANG": {
      "required": true
    },
    "RAILS_ENV": {
      "required": true
    },
    "RAILS_LOG_TO_STDOUT": {
      "required": true
    },
    "RAILS_SERVE_STATIC_FILES": {
      "required": true
    },
    "SECRET_KEY_BASE": {
      "required": true
    }
